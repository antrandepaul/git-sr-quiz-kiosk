{
  "name": "quiz-kiosk",
  "version": "1.0.0",
  "description": "This is SR Git Node quiz kiosk project for Adobe Summit 2018",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'",
    "start": "npm run watch:all",
    "clean": "rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antrandepaul/git-sr-quiz-kiosk.git"
  },
  "author": "An Tran",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antrandepaul/git-sr-quiz-kiosk/issues"
  },
  "homepage": "https://github.com/antrandepaul/git-sr-quiz-kiosk#readme",
  "devDependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "font-awesome": "^4.7.0",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "lite-server": "^2.3.0",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "grunt-bootstrap": "^0.1.0"
  }
}
